          <hashTree>
            <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="JSR223 PreProcessor" enabled="true">
              <stringProp name="scriptLanguage">javascript</stringProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="cacheKey"></stringProp>
              <stringProp name="script">var dateFormat = function(d) {
    var dd = d.getDate();
    var mm = d.getMonth() + 1; //January is 0!
    var yyyy = d.getFullYear();
    if(dd &lt; 10) { dd = &apos;0&apos; + dd; }
    if(mm &lt; 10) { mm = &apos;0&apos; + mm; }
    return yyyy + &apos;-&apos; + mm + &apos;-&apos; + dd;
};

// Current date
var currentDate = new Date();

// Generate a random number of days (up to 365) to subtract
var pastYear = 365;
var daysToSubtract = Math.floor(Math.random() * pastYear);

// Subtract the random number of days from the current date
currentDate.setDate(currentDate.getDate() - daysToSubtract);

// Format the random date
var randomDate = dateFormat(currentDate);

// Set the random date in a JMeter variable
vars.put(&quot;randomDate&quot;, randomDate);
</stringProp>
